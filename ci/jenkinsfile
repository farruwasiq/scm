pipeline{
    agent any
        
    
    stages{
        stage('Git checkout'){
            steps{
                git branch: 'main', url: 'https://github.com/farruwasiq/scm.git'

            }
            
        }
        stage('shell testing'){
            steps{
                sh 'whoami'

            }
            
        }
        
    }
    
}